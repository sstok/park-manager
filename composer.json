{
    "name": "park-manager/park-manager",
    "description": "Open-source hosting solution",
    "license": "MPL-2.0",
    "type": "project",
    "keywords": [
        "ftp",
        "mail",
        "server",
        "hosting"
    ],
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "homepage": "http://www.park-manager.com/",
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "ext-sodium": "*",
        "babdev/pagerfanta-bundle": "^4.4",
        "beberlei/assert": "^3.0.0",
        "doctrine/dbal": "^3.3.7",
        "doctrine/doctrine-bundle": "^2.2.3",
        "doctrine/orm": "^2.9",
        "dragonmantank/cron-expression": "^3.3",
        "egulias/email-validator": "^4.0",
        "jeremykendall/php-domain-parser": "^6.0",
        "league/flysystem": "^2.2",
        "mlocati/ip-lib": "^1.17",
        "mlocati/ocsp": "^1.0",
        "nesbot/carbon": "^3.0",
        "pagerfanta/twig": "^3.1",
        "paragonie/constant_time_encoding": "^2.0",
        "paragonie/halite": "^4.4.2",
        "psr/simple-cache": "^1.0",
        "rollerworks/split-token": "^1.0",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^2.4",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/lock": "*",
        "symfony/mailer": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/uid": "*",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "*",
        "symfony/yaml": "*",
        "twig/extra-bundle": "^3.2",
        "twig/intl-extra": "^3.2",
        "twig/string-extra": "^3.2"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "fakerphp/faker": "^1.13",
        "jangregor/phpstan-prophecy": "^1.0",
        "league/flysystem-memory": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^5.1",
        "php-cs-fixer/accessible-object": "^1.1",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.8.2",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.4.2",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/panther": "^v2.0",
        "symfony/phpunit-bridge": "*",
        "symfony/stopwatch": "*",
        "symfony/var-dumper": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ParkManager\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.0"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
