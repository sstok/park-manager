{
    "name": "park-manager/park-manager",
    "description": "Open-source hosting solution",
    "license": "MPL-2.0",
    "type": "project",
    "keywords": [
        "ftp",
        "mail",
        "server",
        "hosting"
    ],
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "homepage": "http://www.park-manager.com/",
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "ext-sodium": "*",
        "babdev/pagerfanta-bundle": "^4.4",
        "beberlei/assert": "^3.0.0",
        "dragonmantank/cron-expression": "^3.3",
        "league/flysystem": "^2.2",
        "lifthill/lifthill": "^1.0",
        "nesbot/carbon": "^3.0",
        "pagerfanta/twig": "^4.3",
        "rollerworks/pdb-symfony-bridge": "^1.0",
        "rollerworks/pdb-validator": "^1.0",
        "rollerworks/search": ">2.0-BETA4",
        "rollerworks/search-bundle": "^2.0@beta",
        "rollerworks/search-symfony-validator": "^2.0@beta",
        "rollerworks/split-token": "^1.0",
        "rollerworks/x509-validator-symfony": "^0.1.0",
        "symfony/asset": "^7.0",
        "symfony/asset-mapper": "^7.0",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^2.4",
        "symfony/http-client": "*",
        "symfony/lock": "*",
        "symfony/mailer": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/runtime": "*",
        "symfony/twig-bundle": "*",
        "symfony/webpack-encore-bundle": "*",
        "symfony/yaml": "*",
        "symfonycasts/tailwind-bundle": "^0.6.0",
        "twig/extra-bundle": "^3.2",
        "twig/intl-extra": "^3.2",
        "twig/string-extra": "^3.2",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "fakerphp/faker": "^1.13",
        "league/flysystem-memory": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^5.1",
        "php-cs-fixer/accessible-object": "^1.1",
        "phpspec/prophecy-phpunit": "^2.0",
        "rollerscapes/standards": "^1.0",
        "phpunit/phpunit": "^10.5",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/panther": "^2.0",
        "symfony/phpunit-bridge": "*",
        "symfony/stopwatch": "*",
        "symfony/var-dumper": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../lifthill/lifthill/"
        },
        {
            "type": "path",
            "url": "../../rollerworks/search/lib/core"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ParkManager\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.0"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
